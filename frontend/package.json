{
  "name": "tiger-id-frontend",
  "private": true,
  "version": "2.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:auth": "playwright test auth-flow",
    "test:e2e:investigation": "playwright test investigation-flow",
    "test:e2e:tiger": "playwright test tiger-management",
    "test:e2e:facility": "playwright test facility-management",
    "test:e2e:verification": "playwright test verification-flow",
    "test:e2e:discovery": "playwright test discovery-flow",
    "test:e2e:visual": "playwright test tests/visual",
    "test:e2e:visual:update": "playwright test tests/visual --update-snapshots",
    "test:e2e:visual:ui": "playwright test tests/visual --ui",
    "test:e2e:visual:headed": "playwright test tests/visual --headed",
    "test:e2e:accessibility": "playwright test accessibility",
    "test:e2e:report": "playwright show-report",
    "test:e2e:shard1": "playwright test --shard=1/4",
    "test:e2e:shard2": "playwright test --shard=2/4",
    "test:e2e:shard3": "playwright test --shard=3/4",
    "test:e2e:shard4": "playwright test --shard=4/4",
    "test:e2e:sharded": "node run-sharded-tests.js",
    "test:e2e:sharded:parallel": "node run-sharded-tests.js --parallel",
    "test:e2e:merge-reports": "playwright merge-reports --reporter html ./all-blob-reports"
  },
  "dependencies": {
    "@headlessui/react": "^2.1.8",
    "@heroicons/react": "^2.1.5",
    "@hookform/resolvers": "^3.9.0",
    "@reduxjs/toolkit": "^2.2.7",
    "axios": "^1.7.7",
    "clsx": "^2.1.1",
    "date-fns": "^3.6.0",
    "leaflet": "^1.9.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-force-graph-2d": "^1.29.0",
    "react-hook-form": "^7.53.0",
    "react-leaflet": "^4.2.1",
    "react-markdown": "^9.0.1",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.26.2",
    "recharts": "^2.12.7",
    "socket.io-client": "^4.7.5",
    "tailwind-merge": "^2.5.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.11.1",
    "@eslint/js": "^9.9.0",
    "@playwright/test": "^1.47.2",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/leaflet": "^1.9.21",
    "@types/node": "^25.2.0",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/ui": "^2.1.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.12",
    "jsdom": "^28.0.0",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.6",
    "tailwindcss": "^3.4.12",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.0.0",
    "vite": "^5.4.6",
    "vitest": "^2.1.1"
  }
}
